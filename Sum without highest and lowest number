function sumArray(array){

  if(array == null || array.length>=0 && array.length<=2)
  {
    return 0;
  }
  else
  {  
    let sum=0;
    let sortednumbers = array.sort((a, b) => a - b);
    sortednumbers.pop()
    sortednumbers.shift()
    sortednumbers.map((m) => sum+=m)  
    return  sum
  }

}
